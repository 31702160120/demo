<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.a2233.demo">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="聊天室"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.a2233.MainActivity" android:label="聊天室">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:parentActivityName="com.example.a2233.MainActivity"
            android:name="com.example.a2233.register"
            android:label="注册"
            tools:ignore="DuplicateActivity" />
        <activity android:name="com.example.a2233.Homepage" android:label="聊天室" />
    </application>

</manifest>